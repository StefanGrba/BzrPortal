@model BZRForumMedia.Server.ViewModels.VrstaPropisaViewModel

@{
    ViewData["Title"] = "Izmeni vrstu propisa";
}
<div class="container">
<h1>Izmeni vrstu propisa</h1>

<h4>Izmenite polje</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="EditVrstaPropisa">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Naziv" class="control-label"></label>
                <input asp-for="Naziv" class="form-control" />
                <span asp-validation-for="Naziv" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Sačuvaj" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="ListVrstePropisa">Nazad na listu</a>
</div>

</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

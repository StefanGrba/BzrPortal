@model IEnumerable<BZRForumMedia.Server.Models.Video>
    <!-- inner page banner -->
<div id="inner_banner" class="section inner_banner_section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">
                <div class="full">
                    <div class="title-holder">
                        <div class="title-holder-cell text-left">
                            <h1 class="page-title">Video arhiva</h1>
                            <ol class="breadcrumb">
                                <li><a asp-action="Index" asp-controller="Home">Početna</a></li>
                                <li class="active">Video arhiva</li>
                            </ol>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- end inner page banner -->
<!-- section -->
<div class="section padding_layout_1 service_list">
    <div class="container">
        <div class="row">
            @foreach (var video in Model)
            {
                <div class="col-md-4 service_blog margin_bottom_50">
                    <div class="full">
                        <div class="service_img"> <iframe width="350" height="350" src="@video.VideoPath" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe> </div>
                        <div class="service_cont">
                            <h3 class="service_head">@video.Naslov</h3>
                        </div>
                    </div>
                </div>
            }
        </div>
    </div>
</div>